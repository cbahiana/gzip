            <fieldset name="basic">
                <fields name="gzip">
                    <field name="debug" label="PLG_GZIP_FIELD_DEBUG_LABEL" description="PLG_GZIP_FIELD_DEBUG_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                </fields>
            </fieldset>
            <fieldset name="http_cache" label="HTTP" addfieldpath="/plugins/system/gzip/fields">
                <field type="spacer" label="Configure HTTP" />
                <field type="spacer" hr="true" />
                <fields name="gzip">
                    <field type="spacer" label="HTTP Push" />
                    <field name="h2push" label="PLG_GZIP_FIELD_H2PUSH_LABEL" description="PLG_GZIP_FIELD_H2PUSH_DESCRIPTION" type="list" default="" multiple="true">
                        <option value="all">PLG_GZIP_FIELD_H2PUSH_OPTION_ALL</option>
                        <option value="css">PLG_GZIP_FIELD_H2PUSH_OPTION_CSS</option>
                        <option value="js">PLG_GZIP_FIELD_H2PUSH_OPTION_JS</option>
                        <option value="image">PLG_GZIP_FIELD_H2PUSH_OPTION_IMAGE</option>
                        <option value="fonts">PLG_GZIP_FIELD_H2PUSH_OPTION_FONT</option>
                        <option value="media">PLG_GZIP_FIELD_H2PUSH_OPTION_MEDIA</option>
                    </field>
                    <field type="spacer" hr="true" />
                    <field type="spacer" label="HTTP Cache" />
                    <field name="cachefiles" label="PLG_GZIP_FIELD_CACHEFILES_LABEL" description="PLG_GZIP_FIELD_CACHEFILES_DESCRIPTION" type="radio" class="btn-group" default="1">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="hashfiles" label="PLG_GZIP_FIELD_HASHFILES_LABEL" description="PLG_GZIP_FIELD_HASHFILES_DESCRIPTION" type="radio" class="btn-group" default="time">
                        <option value="time">PLG_GZIP_FIELD_HASHFILES_OPTION_TIME</option>
                        <option value="content">PLG_GZIP_FIELD_HASHFILES_OPTION_CONTENT</option>
                    </field>
                    <field name="cache_key" label="PLG_GZIP_FIELD_CACHE_KEY_LABEL" description="PLG_GZIP_FIELD_CACHE_KEY_DESCRIPTION" type="text" default="7BOCz" size="5" pattern="[a-zA-Z0-9@_]+" maxlength="5" class="form-control"/>
                    <field name="maxage" label="PLG_GZIP_FIELD_MAXAGE_LABEL" description="PLG_GZIP_FIELD_MAXAGE_DESCRIPTION" size="3" first="1" last="24" step="1" type="maxage" class="form-control" default="2months"/>
                    <field type="spacer" hr="true" />
                    <field type="spacer" label="Misc" />
                    <field name="cdn_cors" label="PLG_GZIP_FIELD_CDN_CORS_LABEL" description="PLG_GZIP_FIELD_CDN_CORS_DESCRIPTION" type="radio" default="1" class="btn-group">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="mimetypes_custom" label="PLG_GZIP_FIELD_MIMETYPES_CUSTOM_LABEL" hint="PLG_GZIP_FIELD_CDN_MIMETYPES_CUSTOM_HINT" description="PLG_GZIP_FIELD_CDN_MIMETYPES_CUSTOM_DESCRIPTION" type="textarea" />
                    <field name="parse_url_attr" type="textarea" label="PLG_GZIP_FIELD_PARSE_URL_ATTR_LABEL" description="PLG_GZIP_FIELD_PARSE_URL_ATTR_DESCRIPTION"/>
                </fields>
            </fieldset>
            <fieldset name="cdn" label="CDN">
                <fields name="gzip">
                    <field name="cnd_enabled" label="PLG_GZIP_FIELD_CDN_ENABLED_LABEL" description="PLG_GZIP_FIELD_CDN_ENABLED_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="cdn_access_control_origin" label="PLG_GZIP_FIELD_CDN_ACCESS_CONTROL_ORIGIN_LABEL" type="text" description="PLG_GZIP_FIELD_CDN_CDN_ACCESS_CONTROL_ORIGIN_DESCRIPTION" default="*" />
                    <field name="cdn_redirect" label="PLG_GZIP_FIELD_CDN_REDIRECT_LABEL" type="text" description="PLG_GZIP_FIELD_CDN_REDIRECT_DESCRIPTION" />
                    <fields name="cdn">
                        <field name="1" label="PLG_GZIP_FIELD_CDN_0_LABEL" description="PLG_GZIP_FIELD_CDN_0_DESCRIPTION" type="text" />
                        <field name="2" label="PLG_GZIP_FIELD_CDN_1_LABEL" description="PLG_GZIP_FIELD_CDN_1_DESCRIPTION" type="text" />
                        <field name="3" label="PLG_GZIP_FIELD_CDN_2_LABEL" description="PLG_GZIP_FIELD_CDN_2_DESCRIPTION" type="text" />
                    </fields>
                    <field name="cdntypes_custom" label="PLG_GZIP_FIELD_CDN_TYPES_CUSTOM_LABEL" hint="PLG_GZIP_FIELD_CDN_TYPES_CUSTOM_HINT" description="PLG_GZIP_FIELD_CDN_TYPES_CUSTOM_DESCRIPTION" type="textarea" />
