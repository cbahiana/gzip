                </fields>
            </fieldset>
            <fieldset name="javascript" label="Javascript">
                <field type="spacer" label="Configure Javascript settings" />
                <field type="spacer" hr="true" />
                <fields name="gzip">
                    <field name="fetchjs" label="PLG_GZIP_FIELD_FETCHJS_LABEL" description="PLG_GZIP_FIELD_FETCHJS_DESCRIPTION" type="radio" class="btn-group" default="1">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="mergejs" label="PLG_GZIP_FIELD_MERGEJS_LABEL" description="PLG_GZIP_FIELD_MERGEJS_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="minifyjs" label="PLG_GZIP_FIELD_MINIFYJS_LABEL" description="PLG_GZIP_FIELD_MINIFYJS_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="jsignore" label="PLG_GZIP_FIELD_JSIGNORE_LABEL" 
                        description="PLG_GZIP_FIELD_JSIGNORE_DESCRIPTION"  
                        hint="PLG_GZIP_FIELD_JSIGNORE_HINT" 
                        type="textarea" rows="15" cols="45" />
                    <field name="jsremove" label="PLG_GZIP_FIELD_JSREMOVE_LABEL" description="PLG_GZIP_FIELD_JSREMOVE_DESCRIPTION" type="textarea" rows="15" cols="45" />
                </fields>
            </fieldset>
            <fieldset name="css" label="CSS">
                <field type="spacer" label="Configure Css settings" />
                <field type="spacer" hr="true" />
                <fields name="gzip">
                    <field name="fontdisplay" label="PLG_GZIP_FIELD_FONTDISPLAY_LABEL" description="PLG_GZIP_FIELD_FONTDISPLAY_DESCRIPTION" type="list" default="swap">
                        <option value="swap">PLG_GZIP_FIELD_FONTDISPLAY_OPTION_SWAP</option>
                        <option value="fallback">PLG_GZIP_FIELD_FONTDISPLAY_OPTION_FALLBACK</option>
                        <option value="optional">PLG_GZIP_FIELD_FONTDISPLAY_OPTION_OPTIONAL</option>
                        <option value="block">PLG_GZIP_FIELD_FONTDISPLAY_OPTION_BLOCK</option>
                        <option value="auto">PLG_GZIP_FIELD_FONTDISPLAY_OPTION_AUTO</option>
                    </field>
                    <field name="fetchcss" label="PLG_GZIP_FIELD_FETCHCSS_LABEL" description="PLG_GZIP_FIELD_FETCHCSS_DESCRIPTION" type="radio" class="btn-group" default="1">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="mergecss" label="PLG_GZIP_FIELD_MERGECSS_LABEL" description="PLG_GZIP_FIELD_MERGECSS_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="minifycss" label="PLG_GZIP_FIELD_MINIFYCSS_LABEL" description="PLG_GZIP_FIELD_MINIFYCSS_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="asynccss" label="PLG_GZIP_FIELD_ASYNCCSS_LABEL" description="PLG_GZIP_FIELD_ASYNCSS_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="cssignore" label="PLG_GZIP_FIELD_CSSIGNORE_LABEL" 
                    description="PLG_GZIP_FIELD_CSSIGNORE_DESCRIPTION"  
                    hint="PLG_GZIP_FIELD_CSSIGNORE_HINT" 
                    type="textarea" rows="15" cols="45" />
                    <field name="cssremove" label="PLG_GZIP_FIELD_CSSREMOVE_LABEL" description="PLG_GZIP_FIELD_CSSREMOVE_DESCRIPTION" type="textarea" rows="15" cols="45" />
                </fields>
            </fieldset>
            <fieldset name="images" label="Images">
                <field type="spacer" label="Configure images settings" />
                <field type="spacer" hr="true" />
                <fields name="gzip">
                    <field name="imageenabled" label="PLG_GZIP_FIELD_IMAGE_ENABLED_LABEL" description="PLG_GZIP_FIELD_IMAGE_ENABLED_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="imagedimensions" label="PLG_GZIP_FIELD_IMAGE_DIMENSIONS_LABEL" description="PLG_GZIP_FIELD_IMAGE_DIMENSIONS_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="imageremote" label="PLG_GZIP_FIELD_IMAGE_REMOTE_LABEL" description="PLG_GZIP_FIELD_IMAGE_REMOTE_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="imageconvert" label="PLG_GZIP_FIELD_IMAGE_CONVERT_LABEL" 
                            description="PLG_GZIP_FIELD_IMAGE_CONVERT_DESCRIPTION" 
                            hint="PLG_GZIP_FIELD_IMAGE_CONVERT_HINT" 
                            type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>                    
                    <field name="imageignore" label="PLG_GZIP_FIELD_IMAGEIGNORE_LABEL" 
                        description="PLG_GZIP_FIELD_IMAGEIGNORE_DESCRIPTION" 
                        hint="PLG_GZIP_FIELD_IMAGEIGNORE_HINT" 
                        type="textarea" rows="15" cols="45" />
                    <field name="imagesresizestrategy" label="PLG_GZIP_FIELD_IMAGE_CROP_STRATEGY_LABEL"
                           description="PLG_GZIP_FIELD_IMAGE_CROP_STRATEGY_DESCRIPTION" type="list" default="CROP_CENTER">
                        <option value="CROP_DEFAULT">JOPTION_IMAGE_RESIZE_CROP_DEFAULT</option>
                        <option value="CROP_CENTER">JOPTION_IMAGE_RESIZE_CROP_CENTER</option>
                        <option value="CROP_ENTROPY">JOPTION_IMAGE_RESIZE_CROP_ENTROPY</option>
                        <option value="CROP_FACE">JOPTION_IMAGE_RESIZE_CROP_FACE</option>
                    </field>
                    <field type="spacer" hr="true" />
                    <field name="imagesvgplaceholder" label="PLG_GZIP_FIELD_IMAGE_SVG_PLACE_HOLDER_LABEL" description="PLG_GZIP_FIELD_IMAGE_SVG_PLACE_HOLDER_DESCRIPTION" type="list" default="0">
                        <option value="">JNONE</option>
                        <option value="svg">JOPTION_IMAGESVGPLACEHOLDER_SVG</option>
                        <option value="lqip">JOPTION_IMAGESVGPLACEHOLDER_LQIP</option>
                    </field>
                    <field type="spacer" hr="true" />
                    <field name="imageresize" label="PLG_GZIP_FIELD_IMAGE_RESIZE_LABEL" description="PLG_GZIP_FIELD_IMAGE_RESIZE_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="sizes" label="PLG_GZIP_FIELD_IMAGE_SIZES_LABEL" multiple="multiple" description="PLG_GZIP_FIELD_IMAGE_SIZES_DESCRIPTION" type="list" default="">
                        <option value="1920">1920px</option>
                        <option value="1024">1024px</option>
                        <option value="768">768px</option>
                        <option value="320">320px</option>
                    </field>
                    <field type="spacer" hr="true" />
                    <field name="imagecssresize" label="PLG_GZIP_FIELD_IMAGE_CSS_RESIZE_LABEL" description="PLG_GZIP_FIELD_IMAGE_CSS_RESIZE_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="css_sizes" label="PLG_GZIP_FIELD_IMAGE_CSS_SIZES_LABEL" multiple="multiple" description="PLG_GZIP_FIELD_IMAGE_CSS_SIZES_DESCRIPTION" type="list" default="">
                        <option value="1920">1920px</option>
                        <option value="1024">1024px</option>
                        <option value="768">768px</option>
                        <option value="320">320px</option>
                    </field>
                </fields>
            </fieldset>
            <fieldset name="pwa_app" label="PWA::App">
                <field type="spacer" label="Configure progressive web application settings" />
                <field type="spacer" hr="true" />
                <fields name="gzip">
                    <field name="debug_pwa" label="PLG_GZIP_FIELD_PWA_DEBUG_LABEL" description="PLG_GZIP_FIELD_PWA_DEBUG_DESCRIPTION" type="radio" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                    </field>
                    <field name="pwaenabled" label="PLG_GZIP_FIELD_PWA_ENABLED_LABEL" description="PLG_GZIP_FIELD_PWA_ENABLED_DESCRIPTION" type="list" class="btn-group" default="0">
                        <option value="1">JYes</option>
                        <option value="0">JNo</option>
                        <option value="-1">PLG_GZIP_FIELD_PWA_ENABLED_OPTION_FORCE_UNINSTALL</option>
